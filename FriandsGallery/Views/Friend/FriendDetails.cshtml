@model FriandsGallery.Models.FriendModel;
<div>
    <h1>@Model.FullName</h1>
    <div class="image-container">
        @foreach (var imageModel in Model.Images)
        {
            @if (imageModel.Data != null)
            {
                <img class="friend-image"
                    src="data:image/jpeg;base64,@Convert.ToBase64String(imageModel.Data)" />
            }
        }
    </div>
</div>